{
  "name": "@robin-rossow/vue-input-number",
  "version": "3.0.0",
  "keywords": [
    "component",
    "currency",
    "input",
    "text",
    "number",
    "numeric",
    "vue3",
    "vue",
    "typescript"
  ],
  "type": "module",
  "private": false,
  "files": [
    "dist"
  ],
  "main": "./dist/vue-input-number.umd.cjs",
  "module": "./dist/vue-input-number.js",
  ".": {
    "import": {
      "types": "./dist/index.d.ts",
      "default": "./dist/vue-input-number.js"
    },
    "require": {
      "types": "./dist/index.d.ts",
      "default": "./dist/vue-input-number.umd.cjs"
    }
  },
  "types": "./dist/index.d.ts",
  "scripts": {
    "dev": "vite",
    "test": "vitest run --coverage",
    "build": "vite build && vue-tsc --emitDeclarationOnly",
    "build-gh-pages": "vite build --config vite-gh-pages.config.ts",
    "preview": "vite preview"
  },
  "dependencies": {
    "accounting-js": "^2.0.3",
    "vue": "^3.5.25"
  },
  "devDependencies": {
    "@types/node": "^24.10.1",
    "@vitejs/plugin-vue": "^6.0.2",
    "@vitest/coverage-v8": "^4.0.18",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.8.1",
    "happy-dom": "^20.6.1",
    "typescript": "~5.9.3",
    "vite": "^7.3.1",
    "vitest": "^4.0.18",
    "vue-tsc": "^3.2.4"
  }
}
